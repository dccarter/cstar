//import "stdlib/path.cxy"


exception AgeError(msg: String) => msg.str()

struct User {
    - name: String;
    - age = 16:i32;
    func `init`(@unused name: String) {
        this.name = &&name
    }

    func drink() : !void {
        if (age < 21)
            raise AgeError(f"${name} is under age")
    }
}

func main(): !void {
    var user = User("Carter");
    user.drink()
}
