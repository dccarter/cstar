struct User {
    - name: String;
    - age = 35:i32;
    func `init`(@unused name: String) {
        this.name = &&name
    }
}

func say(user: User) {
    return user.name
}

func main(args: [string]) {
    var user = User("Carter");
    var x = stackAlloc!(String, "Carter");
}
