import { println } from "stdlib/io.cxy"
import { HashMap } from "stdlib/map.cxy"
import { StringBuilder } from "stdlib/strlib.cxy"


func main(args: [string]) {
    var map = HashMap[string, i32]();
    map.["Carter"] = 10
    map.["Megan"] = 12
    for (const i : 0..20) {
        map.[f"${i}"] = <i32>i
    }

    var carter = map.["Carter"];
    println(f"${carter}")

    for (const (key, value): map) {
        println(f"${key} -> ${value}")
    }

    for (const i : 6..20) {
        map.remove(f"${i}")
    }
    println("-------------------------------------------------");
    for (const (key, value): map) {
        println(f"${key} -> ${value}")
    }
}