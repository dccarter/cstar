// tuple declaration
type Tup1 = (bool, wchar)
type Tup2 = (wchar, (f32, string, Tup1))

func main(args: [string]) {
    // tuple literals
    // NOTE (x) is not a tuple literal
    (true, 10);
    ('a', true, 10, "Hello");
    ('a', ("World", 32, (true, 5.996)));

    // Tuple type checking
    var x: (bool, i32) = (true, 10);
    x = (false, 20)
    var y : Tup2 = ('a', (0.6, "Hello", (true, 'A')));
}