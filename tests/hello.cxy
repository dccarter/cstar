import { println } from "stdlib/io.cxy"

async func hello(name: string, age: i32) {
    const start = now();
    asleep(1000 + 100*age)
    println(now()-start, " - Hello ", name, ", You are ", age, " yrs old!")
}

func main(args: [string]) {
    for (const i: 22..44)
        @go hello("John", <i32>i)
    asleep(5000);
}
