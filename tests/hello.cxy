import { print, println, fdRead, STDIN } from "stdlib/io.cxy"

struct Thing {
    id: i32
    func print(x: i32, ...args: auto) {
        println(id, " ", x, " ", ...args)
    }
}

func many(x: i32, ...args: auto) {
    println(x, ...args)
}

func main(args: [string]) {
    const name = "Carter";
    println(10, 20, "Carters");
    many(1, 2, name, Thing{id: 12});

    Thing{id: 10}.print(20, name)
}
