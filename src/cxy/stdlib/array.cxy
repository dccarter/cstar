module array

import "stdlib/utils.cxy"

pub struct Array[T] {
    - data: [T]
    - len: u64

    - func grow() {
        var capacity = len!(data);
        if (len < capacity) {
            capacity += (capacity / 2)
            data = utils.reallocSlice[T](data, capacity)
        }
    }

    func `new`(initialSize: u64) {
        data = utils.allocSlice[T](initialSize)
    }
}
